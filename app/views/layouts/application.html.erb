<!DOCTYPE html>
<html>
<head>
 <meta http-equiv="Content-type" content="text/html; charset=utf-8" />  
<title><%= title %></title>
<%= csrf_meta_tag %>
<%= render 'layouts/stylesheets' %>
<%= javascript_include_tag :defaults %>
</head>
<div class="container">


<div id="top_panel">
<div id="logo">
	<%= render 'layouts/logo' %>
</div>
<div id="banner">
	<%= render 'layouts/banner' %>
	</div>
<div id="login_panel">	
	<%= render 'layouts/header' %>
	</div>
	</div>



<div id="left_panel">
	<div id="nav_panel">
			<%= render 'layouts/navigation' %>
	</div>
	
	<div id="chat_panel">
	</div>
</div>	
	
<div id="board_panel">
		<div id="board">

	<%= yield %>
			</div>
		<div id="variations">
			</div>
</div>

<div id="right_panel">	
	<div id="game_info_panel">
	</div>
	
	<div id="game_notation_panel">
	</div>
</div>

<div id="footer">
	<%= render 'layouts/footer' %>
</div>

<%= debug(params) if Rails.env.development? %>



</div>
</body>
</html>